# Ignore dependency manager configuration
/.bundle
.yarn-integrity

# Ignore installed dependencies
/node_modules
/vendor

# Ignore logfiles
/log/*
!/log/.keep
/yarn-error.log
.byebug_history
yarn-debug.log*

# Ignore tempfiles
/public/uploads/*
!/public/uploads/.keep
/tmp/*
!/tmp/.keep
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore build artefacts
/public/assets
/public/packs
/public/packs-test

# Ignore encryption secrets
/config/master.key
/config/credentials/*.key

# Ignore OS specific files
.DS_Store

# Ignore editor artefacts and configuration
*.swap
/tags

/config/credentials/development.key
